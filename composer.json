{
    "name": "seblhaire/uploader",
    "description": "A Laravel library to provide file upload utilities. A Javascript library builds a complete file upload widget with upload button, drag-and-drop zone, progress bar and result builder. A controller is available to manage uploaded files.",
    "type": "library",
    "authors": [
        {
            "name": "SÃ©bastien L'haire",
            "email": "sebastien@lhaire.org",
            "homepage": "http://sebastien.lhaire.org/"
        }
    ],
    "homepage": "https://github.com/seblhaire/uploader",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/http": "^11.0",
        "danielstjules/stringy": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Seblhaire\\Uploader\\": "src/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Seblhaire\\Uploader\\UploaderServiceProvider"
          ],
          "aliases": {
              "UploaderHelper": "Seblhaire\\Uploader\\UploaderHelper"
          }
      }
    }
}
